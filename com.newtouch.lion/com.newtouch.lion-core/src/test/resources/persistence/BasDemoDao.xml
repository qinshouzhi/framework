<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.newtouch.lion.mapper.BasDemoDao">
	<resultMap id="BaseResultMap" type="com.newtouch.lion.model.BasDemo">
		<id property="id" column="bas_demo_id" jdbcType="INTEGER" />
		<result property="parentId" column="parent_id" jdbcType="INTEGER" />
		<result property="nameZh" column="name_zh" jdbcType="VARCHAR" />
		<result property="createdById" column="CREATE_BY" jdbcType="INTEGER" />
		<result property="createdDate" column="CREATE_DATE" jdbcType="TIMESTAMP" />
		<result property="updatedById" column="UPDATE_BY" jdbcType="INTEGER" />
		<result property="updatedDate" column="UPDATE_DATE" jdbcType="TIMESTAMP" />
	</resultMap>
	<!-- 插件数据 -->
	<insert id="insert" parameterType="com.newtouch.lion.model.BasDemo">
		INSERT INTO
		bas_demo(bas_demo_id,parent_id,name_zh,CREATE_BY,CREATE_DATE,UPDATE_BY,UPDATE_DATE)
		VALUES(
		#{id,jdbcType=INTEGER},
		#{parentId,jdbcType=INTEGER},
		#{nameZh,jdbcType=VARCHAR},
		#{createdById,jdbcType=INTEGER},
		#{createdDate,jdbcType=DATE},
		#{updatedById,jdbcType=INTEGER},
		#{updatedDate,jdbcType=DATE})
	</insert>
	<!-- 根据ID查找 -->
	<select id="getById" parameterType="INTEGER" resultMap="BaseResultMap">
		SELECT *
		FROM bas_demo WHERE bas_demo_id = #{id}
	</select>
	<!--#{id}引用参数id的值 -->
	<delete id="delete" parameterType="INTEGER" >
		delete from bas_demo where bas_demo_id=#{id}
	</delete>
</mapper>